git config --global user.name "ane"
git config --global user.email rkfbd@naver.com
git config --global --bool pull.rebase true
git config --global push.default simple

git clone git@github.com:ane/manageiq.git # Use "-o my_fork" if you don't want the remote to be named origin
cd manageiq
git remote add upstream git@github.com:ManageIQ/manageiq.git
git fetch upstream
